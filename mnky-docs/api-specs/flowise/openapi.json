  "info": {
    "description": "This OpenAPI specification defines the comprehensive REST API interface for Flowise, customized for integration into the MOOD MNKY agent ecosystem. It includes endpoints for chatflow execution, flow management, node schema discovery, credential operations, deployment handling, and system metadata. Designed with first-principles engineering, this spec enables robust AI-assisted automation, integration with Supabase, n8n, LangChain, and CODE MNKY's virtual agents.",
    "title": "Flowise API - MOOD MNKY Integration",
    "version": "1.0.0"
  },
  "openapi": "3.0.3",
  "paths": {
    "/api/v1/chatflows": {
      "get": {
        "operationId": "getChatflows",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "type": "object"
                  },
                  "type": "array"
                }
              }
            },
            "description": "A list of all chatflows"
          }
        },
        "summary": "List all chatflows",
        "tags": [
          "Chatflows"
        ]
      },
      "post": {
        "operationId": "createChatflow",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object"
              }
            }
          },
          "description": "Chatflow object",
          "required": true
        },
        "responses": {
          "201": {
            "description": "Chatflow created successfully"
          }
        },
        "summary": "Create a new chatflow",
        "tags": [
          "Chatflows"
        ]
      }
    },
    "/api/v1/chatflows/export/{id}": {
      "get": {
        "operationId": "exportChatflow",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            },
            "description": "Chatflow JSON"
          }
        },
        "summary": "Export a chatflow as JSON",
        "tags": [
          "Chatflows"
        ]
      }
    },
    "/api/v1/chatflows/import": {
      "post": {
        "operationId": "importChatflow",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "properties": {
                  "file": {
                    "format": "binary",
                    "type": "string"
                  }
                },
                "type": "object"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Chatflow imported successfully"
          }
        },
        "summary": "Import a chatflow from file or JSON",
        "tags": [
          "Chatflows"
        ]
      }
    },
    "/api/v1/chatflows/{id}": {
      "delete": {
        "operationId": "deleteChatflow",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "204": {
            "description": "Chatflow deleted successfully"
          }
        },
        "summary": "Delete a chatflow",
        "tags": [
          "Chatflows"
        ]
      },
      "get": {
        "operationId": "getChatflowById",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Chatflow object"
          }
        },
        "summary": "Get a specific chatflow by ID",
        "tags": [
          "Chatflows"
        ]
      },
      "put": {
        "operationId": "updateChatflow",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Chatflow updated successfully"
          }
        },
        "summary": "Update a chatflow",
        "tags": [
          "Chatflows"
        ]
      }
    },
    "/api/v1/chatflows/{id}/execute": {
      "post": {
        "operationId": "executeChatflow",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "properties": {
                  "inputs": {
                    "type": "object"
                  }
                },
                "required": [
                  "inputs"
                ],
                "type": "object"
              }
            }
          },
          "description": "User inputs for the flow",
          "required": true
        },
        "responses": {
          "200": {
            "description": "Chatflow executed successfully"
          }
        },
        "summary": "Execute a chatflow with inputs",
        "tags": [
          "Chatflows"
        ]
      }
    },
    "/api/v1/credentials": {
      "get": {
        "operationId": "getCredentials",
        "responses": {
          "200": {
            "description": "List of credentials"
          }
        },
        "summary": "Get all credential sets",
        "tags": [
          "Credentials"
        ]
      },
      "post": {
        "operationId": "createCredential",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object"
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "description": "Credential created successfully"
          }
        },
        "summary": "Create new credential set",
        "tags": [
          "Credentials"
        ]
      }
    },
    "/api/v1/credentials/{id}": {
      "delete": {
        "operationId": "deleteCredential",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "204": {
            "description": "Credential deleted successfully"
          }
        },
        "summary": "Delete a credential set",
        "tags": [
          "Credentials"
        ]
      }
    },
    "/api/v1/deployments": {
      "get": {
        "operationId": "getDeployments",
        "responses": {
          "200": {
            "description": "List of deployed chatflows"
          }
        },
        "summary": "List all deployed chatflows",
        "tags": [
          "Deployments"
        ]
      }
    },
    "/api/v1/nodes": {
      "get": {
        "operationId": "getNodes",
        "responses": {
          "200": {
            "description": "List of nodes"
          }
        },
        "summary": "Get all nodes available for chatflow creation",
        "tags": [
          "Nodes"
        ]
      }
    },
    "/api/v1/system/info": {
      "get": {
        "operationId": "getSystemInfo",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            },
            "description": "System info"
          }
        },
        "summary": "Get instance-level metadata and configuration",
        "tags": [
          "System"
        ]
      }
    }
  },
  "servers": [
    {
      "description": "Development server for Flowise hosted by MOOD MNKY",
      "url": "https://flowise-dev.moodmnky.com"
    }
  ],
  "tags": [
    {
      "description": "Endpoints related to executing and managing chatflows.",
      "name": "Chatflows"
    },
    {
      "description": "Endpoints for retrieving node information and schemas.",
      "name": "Nodes"
    },
    {
      "description": "Endpoints for managing credential sets.",
      "name": "Credentials"
    },
    {
      "description": "Endpoints for managing deployed flows.",
      "name": "Deployments"
    },
    {
      "description": "Endpoints for instance-level metadata and health checks.",
      "name": "System"
    }
  ]
}